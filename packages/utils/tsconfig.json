{
	"extends": "../../tsconfig.json",
	"compilerOptions": {
		"outDir": "dist",
		"rootDir": "src",
		"baseUrl": ".",
		"paths": {
			"@/*": ["./src"]
		},
		"noEmit": false,
		"allowImportingTsExtensions": false,
		"declaration": true,
		"composite": true
	},
	"include": ["src"],
	"exclude": ["node_modules", "dist"]
}
