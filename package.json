{
  "name": "caterly",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "turbo build",
		"dev:ui": "turbo dev --filter=@caterly/ui",
		"dev:web": "turbo dev --filter=@caterly/web",
		"dev:admin": "turbo dev --filter=admin",
    "build:ui": "turbo build --filter=@caterly/ui",
    "build:web": "turbo build --filter=@caterly/web",
		"build:admin": "turbo build --filter=admin",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo check-types",
    "clean": "turbo clean && rm -rf node_modules && rm -rf packages/ui/dist"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.49.4",
    "@tailwindcss/vite": "^4.1.4",
    "@vitejs/plugin-react": "^4.2.1",
    "clsx": "^2.1.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3",
    "tailwindcss": "4.1.4",
    "vite": "^5.1.5",
    "vite-plugin-dts": "^4.5.3",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "devDependencies": {
    "@types/node": "^22.14.1",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@typescript-eslint/eslint-plugin": "^8.31.0",
    "@typescript-eslint/parser": "^8.31.0",
    "eslint": "^9.25.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-sonarjs": "^3.0.2",
    "eslint-plugin-turbo": "^2.5.0",
    "eslint-plugin-unicorn": "^51.0.1",
    "globals": "^16.0.0",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss-animate": "^1.0.7",
    "turbo": "^2.5.0",
    "typescript": "^5.4.2"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}
